SELECT
    Date,
    principal_component_1,
    principal_component_2,
    principal_component_3
FROM ML.PREDICT(
    MODEL `financial-data-storage.clean_data_preprod.modele_acp`,
    (FROM ${ref("clean_data_preprod", "euribor")})
)